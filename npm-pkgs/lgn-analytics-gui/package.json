{
  "name": "@lgn/analytics-gui",
  "version": "0.1.2",
  "description": "Legion's monorepo",
  "repository": "https://github.com/legion-labs/legion",
  "author": "devs@legionlabs.com",
  "license": "MIT OR Apache-2.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@6.24.1",
  "scripts": {
    "start:desktop": "cross-env LOAD_DEV_URL=true concurrently --names \"vite,electron\" \"vite dev\" \"electron src/electron/main.cjs\" --kill-others",
    "start:browser": "svelte-kit dev",
    "start": "pnpm start:browser",
    "build": "svelte-kit build && copyfiles -f src/404.html dist/ && replace-in-file dist/index.html --configFile=githubPagesHackInjection.cjs --quiet",
    "check": "pnpm lint && pnpm fmt:check && pnpm svelte:check",
    "lint": "eslint --ext .ts,.svelte .",
    "lint:fix": "eslint --ext .ts,.svelte --fix .",
    "fmt:check": "prettier --check . --plugin-search-dir=. ./**/*.svelte",
    "fmt": "prettier --write . --plugin-search-dir=. ./**/*.svelte",
    "svelte:check": "tsc && svelte-check --use-new-transformation",
    "clean": "rimraf node_modules dist",
    "test": "vitest run",
    "fluent:generate": "cargo run --bin lgn-fluent-generator -- -i ./src/assets/locales/en-US/**/*.ftl -o ./src/types"
  },
  "devDependencies": {
    "@lgn/vite-plugin-api-codegen": "workspace:^0.1.0",
    "@lgn/vite-plugin-ts-proto": "workspace:^0.1.0",
    "@sveltejs/adapter-static": "1.0.0-next.34",
    "@sveltejs/kit": "1.0.0-next.347",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.47",
    "@swc/core": "^1.2.196",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.2",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/babel__core": "^7.1.19",
    "@types/d3": "^7.4.0",
    "@types/jest": "^28.1.0",
    "@types/node": "^17.0.39",
    "@types/tailwindcss": "^3.0.10",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^19.0.0",
    "node-fetch": "^3.2.6",
    "pnpm": "^7.1.7",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "puppeteer": "^14.2.1",
    "replace-in-file": "^6.3.5",
    "rimraf": "^3.0.2",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.24",
    "ts-proto": "^1.115.1",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.13.1"
  },
  "dependencies": {
    "@fluent/bundle": "^0.17.1",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@lgn/proto-telemetry": "^0.1.0",
    "@lgn/web-client": "^0.1.0",
    "@types/progressbar.js": "^1.1.2",
    "binary-search": "^1.3.6",
    "browser-headers": "^0.4.1",
    "d3": "^7.4.4",
    "date-fns": "^2.28.0",
    "electron": "^19.0.3",
    "electron-serve": "^1.1.0",
    "google-protobuf": "^3.20.1",
    "grpc-web": "^1.3.1",
    "long": "^5.2.0",
    "progressbar.js": "^1.1.0",
    "protobufjs": "^6.11.3",
    "semaphore-async-await": "^1.5.1",
    "svelte-loading-spinners": "^0.1.7",
    "uuid-random": "^1.3.2"
  }
}
