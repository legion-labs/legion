{
  "name": "editor",
  "version": "0.1.0",
  "description": "Legion's monorepo",
  "repository": "https://github.com/legion-labs/legion",
  "author": "devs@legionlabs.com",
  "license": "MIT OR Apache-2.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@6.24.1",
  "scripts": {
    "start": "svelte-kit dev",
    "build": "svelte-kit build",
    "check": "pnpm lint && pnpm fmt:check && pnpm svelte:check",
    "lint": "eslint --ext .ts,.svelte .",
    "lint:fix": "eslint --ext .ts,.svelte --fix .",
    "fmt": "prettier --write . --plugin-search-dir=. ./**/*.svelte",
    "fmt:check": "prettier --check . --plugin-search-dir=. ./**/*.svelte",
    "svelte:check": "tsc && svelte-check --use-new-transformation",
    "clean": "rimraf node_modules dist",
    "test": "vitest run",
    "benchmark": "vite build --mode development -c benchmarks/vite.config.js -l silent && node dist/benchmarks/index.js"
  },
  "devDependencies": {
    "@lgn/vite-plugin-ts-proto": "workspace:^0.1.0",
    "@lgn/vite-plugin-wasm": "workspace:^0.1.0",
    "@sveltejs/adapter-static": "^1.0.0-next.28",
    "@sveltejs/kit": "^1.0.0-next.290",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.39",
    "@swc/core": "^1.2.156",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/svelte": "^3.1.0",
    "@types/color-convert": "^2.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.4",
    "c8": "^7.11.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.3",
    "prettier": "^2.6.0",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.4",
    "tailwindcss": "^3.0.23",
    "ts-proto": "^1.110.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.6.3"
  },
  "dependencies": {
    "@fluent/bundle": "^0.17.1",
    "@fluent/langneg": "^0.6.1",
    "@iconify/svelte": "^2.1.2",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@lgn/proto-editor": "^0.1.0",
    "@lgn/proto-streaming": "^0.1.0",
    "@lgn/web-client": "^0.1.0",
    "@tauri-apps/api": "^1.0.0-rc.2",
    "browser-headers": "^0.4.1",
    "color-convert": "^2.0.1",
    "google-protobuf": "^3.19.4",
    "grpc-web": "^1.3.1",
    "long": "^5.2.0",
    "monaco-editor": "0.33.0",
    "pnpm": "^6.32.3",
    "protobufjs": "^6.11.2",
    "rxjs": "7.5.5",
    "svelte-forms": "^2.3.0",
    "svelte-window": "^1.2.3",
    "uuid": "^8.3.2",
    "uuid-random": "^1.3.2"
  }
