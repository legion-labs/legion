/**
 * A full dictionnary of all the found message ids, attributes, and variables
 */
declare type Fluent = {
  {%- for description in entry_descriptions %}
  "{{ description.id }}": {% if description.attributes.len() == 0 && description.variables.len() == 0 %}null{% else %}{
    {%- if description.attributes.len() > 0 %}
    attributes: (
      {% for attribute in description.attributes -%}
      | "{{ attribute }}"
      {% endfor -%}
    );
    {% endif %}
    {%- if description.variables.len() > 0 %}
    variables: (
      {% for variable in description.variables -%}
      | "{{ variable }}"
      {% endfor -%}
    );
    {% endif -%}
  }{% endif %};
  {%- endfor %}
};
