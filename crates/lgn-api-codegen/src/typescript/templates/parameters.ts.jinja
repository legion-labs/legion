{% for (path, routes) in api.paths %}
    {% for route in routes %}    
        {% if !route.parameters.query.is_empty() -%}
            export type {{ route.name|pascal_case }}Query = {
                {% for parameter in route.parameters.query -%}
                  {% include "parameter.ts.jinja" %}
                {% endfor %}
            };
        {% endif %}
    {% endfor %}
{% endfor %}
