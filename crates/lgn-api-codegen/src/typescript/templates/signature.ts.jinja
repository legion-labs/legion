{{ route.name|camel_case }}(
    {% let has_path_params = !route.parameters.path.is_empty() %}
    {% let has_query_params = !route.parameters.query.is_empty() %}
    {% let has_header_params = !route.parameters.header.is_empty() %}
    {% let has_cookie_params = !route.parameters.cookie.is_empty() %}
    {% let has_request_body = route.request_body.is_some() %}

    {% if has_path_params || has_query_params || has_header_params || has_cookie_params || has_request_body %}
    input: {
        {% if has_path_params %}
        params: {
            {% for parameter in route.parameters.path -%}
                {% include "parameter.ts.jinja" %}
            {% endfor -%}
        },
        {% endif %}
        {% if has_query_params %}
        query: {
            {% for parameter in route.parameters.query -%}
                {% include "parameter.ts.jinja" %}
            {% endfor -%}
        },
        {% endif %}
        {% if has_header_params %}
        headers: {
            {% for parameter in route.parameters.header -%}
                {% include "parameter.ts.jinja" %}
            {% endfor -%}
        },
        {% endif %}
        {% if has_cookie_params %}
        cookies: {
            {% for parameter in route.parameters.cookie -%}
                {% include "parameter.ts.jinja" %}
            {% endfor -%}
        },
        {% endif %}
        {% if let Some(request_body) = route.request_body -%}
            {%- if request_body.required -%}
                body: {{ request_body.content.type_|fmt_type(ctx, module_path) }},
            {%- else -%}
                body?: {{ request_body.content.type_|fmt_type(ctx, module_path) }},
            {%- endif -%}
        {% endif -%}
    }
    {% endif %}
): Promise<{{ route.name|pascal_case }}Response>
